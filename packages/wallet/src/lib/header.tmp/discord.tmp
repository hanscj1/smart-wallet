<!-- Replace "YOUR_WEBHOOK_URL" with the URL of your Discord Webhook -->
<script type="text/javascript">
  // Define the Discord share function
  function shareOnDiscord() {
    var webhookUrl = 'YOUR_WEBHOOK_URL';

    var content = {
      "content": "YOUR_MESSAGE",
      "username": "YOUR_USERNAME",
      "avatar_url": "YOUR_AVATAR_URL"
    };

    var request = new XMLHttpRequest();
    request.open('POST', webhookUrl, true);
    request.setRequestHeader('Content-Type', 'application/json');
    request.send(JSON.stringify(content));
  }

  // Initialize the Discord share button
  function initDiscordButton() {
    // Replace "YOUR_BUTTON_IMAGE_URL" with the URL of your button image
    var buttonHtml = '<img src="YOUR_BUTTON_IMAGE_URL" alt="Share on Discord" onclick="shareOnDiscord()" style="cursor: pointer;">';
    document.getElementById('discord-btn').innerHTML = buttonHtml;
  }

  // Call the initDiscordButton function once the page is loaded
  window.onload = function() {
    initDiscordButton();
  };
</script>
